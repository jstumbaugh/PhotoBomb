# All Vagrant configuration is done below. The "2" in Vagrant.configure
# configures the configuration version (we support older styles for
# backwards compatibility). Please don't change it unless you know what
# you're doing.

Vagrant.configure(2) do |config|
  config.vm.network "private_network", ip: "192.168.10.9"
  config.vm.box = "hashicorp/precise32"
  config.vm.provision :shell, path: "installShellScript.sh"

  config.vm.network "forwarded_port", guest: 80, host: 2000
  config.vm.synced_folder "website/", "/var/www/photobomb"

end
